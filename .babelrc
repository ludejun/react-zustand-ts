{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3
        // "target": {},
        // "modules": false
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    // "@babel/plugin-proposal-class-properties",
    "react-refresh/babel",
    ["@babel/plugin-transform-runtime", { "corejs": 3 }]
  ]
}
